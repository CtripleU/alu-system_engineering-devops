#!/usr/bin/env bash
# configure Nginx to be running and listening on port 80 of all the serverâ€™s active IPv4 IPs
sed -i -e "s/listen 8080 default_server;/listen 80 default_server;/" /etc/nginx/sites-available/default
sed -i 's/8080/80/g' /etc/nginx/sites-enabled/default
sudo service nginx restart
